doctype html
html
  head
    title SumoMe - Survey
    meta(name='viewport', content='initial-scale=1, maximum-scale=1, user-scalable=no')
    meta(name='viewport', content='width=device-width')
    link(href='//sumome.com/favicon.ico', rel='shortcut icon', id='meta-favicon')
    link(href='//ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.css', rel='stylesheet', id='css-angular-material')
    link(href='//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css', rel='stylesheet', id='css-font-awesome')
    link(href='//fonts.googleapis.com/css?family=Maven+Pro:400,500,700,900', rel='stylesheet', id='css-google-font')
    link(href='//sumome.com/css/redesign-app.css', rel='stylesheet', id='css-sumome')
    link(href='//sumome.com/css/redesign.css', rel='stylesheet', id='css-sumome')
    link(href='/css/style.min.css', rel='stylesheet', id='css-custom')
    style.
      @charset "UTF-8";[ng\:cloak],[ng-cloak],[data-ng-cloak],[x-ng-cloak],.ng-cloak,.x-ng-cloak,.ng-hide:not(.ng-hide-animate){display:none !important;}ng\:form{display:block;}.ng-animate-shim{visibility:hidden;}.ng-anchor{position:absolute;}
    block header-css
  body(data-ng-app='surveyApp', data-ng-cloak, layout='column')
    md-toolbar(layout='row')
      .md-toolbar-tools
        .title
          a(href='/')
            img(src='data:image/png;base64,')
            span.hide-xs SumoMe | Survey
        span(flex)
        - if (!user)
          md-button(href='/admin', aria-label='Log In') Log In
        - else
          md-button(href='/admin', aria-label='Admin') Admin
          md-button(href='/logout', aria-label='Log Out') Log Out
    div(flex,layout='row')
      md-content#content(flex)
        block body-content

    script(src='//ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js', id='js-jquery')
    script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js', id='js-angular')
    script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-route.min.js', id='js-angular-route')
    script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-animate.min.js', id='js-angular-animate')
    script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-aria.min.js', id='js-angular-aria')
    script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-messages.min.js', id='js-angular-messages')
    script(src='//ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.js', id='js-angular-material')

    script(src='/js/script.min.js', id='js-custom')
    //- Add livereload to help in local development only
    if !!!((process.env.NODE_ENV || 'local').toLowerCase().indexOf('local'))
      script(src='//127.0.0.1:35729/livereload.js', async, data-id='js-livereload')
    block body-scripts
